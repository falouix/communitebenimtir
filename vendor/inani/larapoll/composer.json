{
    "name": "inani/larapoll",
    "description": "A Laravel package to create polls",
    "keywords": ["polls", "votes", "laravel", "php"],
    "license": "MIT",
    "authors": [{
        "name": "Chedli Elhaj Ali",
        "email": "chedli@cdfcenter.com"
    }],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Inani\\Larapoll\\": "src/"
        }
    },
    "require": {
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Inani\\Larapoll\\LarapollServiceProvider"
            ],
            "aliases": {
                "PollWriter": "Inani\\Larapoll\\PollWriterFacade"
            }
        }
    }
}